    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "postcss-import": "^14.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.32.8",
    "typescript": "^4.3.5",
    "vite": "^2.1.4",
    "vite-plugin-mock": "^2.5.0",
    "vite-plugin-svg-sprite-component": "^1.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
